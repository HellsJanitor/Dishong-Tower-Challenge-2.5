<configs>
	<!-- removes seed crafting skill as it's not required -->
	<set xpath="//crafting_skill[@name='craftingSeeds']/@max_level">1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']//display_entry[1]/@unlock_level">1,1,1,1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']//display_entry[2]/@unlock_level">1,1,1,1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[1]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[2]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[3]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[4]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[5]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[6]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[7]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[8]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[9]/@level">1,1</set>
	<set xpath="//crafting_skill[@name='craftingSeeds']/effect_group//passive_effect[10]/@level">1,1</set>
	
	<!-- increase probabilty of finding vehicle magazines with each perk point -->
	<set xpath="//perk[@name='perkGreaseMonkey']/effect_group//passive_effect[2]/@value">3,15</set>
	<set xpath="//perk[@name='perkGreaseMonkey']/effect_group//passive_effect[3]/@value">3,15</set>	
	
	<!-- Once vehicle crafting skill reaches 50, set the probability bonus of finding vehicle crafting skill magazines to 0 -->
	<set xpath="//perk[@name='perkGreaseMonkey']/effect_group/passive_effect[3]/requirement[@name='ProgressionLevel']/@value">50</set>
	<set xpath="//perk[@name='perkGreaseMonkey']/effect_group/passive_effect[4]/requirement[@name='ProgressionLevel']/@value">50</set>
	
	<!-- lowers number requirement of vehicle magazines to craft vehicles AFTER minibike, up to and including gyrocopter -->
	<set xpath="//crafting_skill[@name='craftingVehicles']/@max_level">50</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[1]/@level">5,50</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[1]/@tags">vehicleWheels</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']//display_entry[@name_key='vehiclesT2']/@unlock_level">15</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[2]/@level">15,50</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[2]/@tags">resourceOil</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']//display_entry[@name_key='vehiclesT3']/@unlock_level">20</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[3]/@level">20,50</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[3]/@tags">ammoGasCan</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']//display_entry[@name_key='vehiclesT4']/@unlock_level">35</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[4]/@level">35,50</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[4]/@tags">resourceAcid,ammoGasCanBundle</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']//display_entry[@name_key='vehiclesT5']/@unlock_level">50</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/effect_group//passive_effect[5]/@level">50,50</set>
	
	<set xpath="//crafting_skill[@name='craftingVehicles']/display_entry[1]/unlock_entry/@item">vehicleWheels</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/display_entry[2]/unlock_entry/@item">resourceOil</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/display_entry[3]/unlock_entry/@item">ammoGasCan</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']/display_entry[4]/unlock_entry/@item">resourceAcid,ammoGasCanBundle</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']//display_entry[1]/@icon">vehicleWheels</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']//display_entry[2]/@icon">resourceOil</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']//display_entry[3]/@icon">ammoGasCan</set>
	<set xpath="//crafting_skill[@name='craftingVehicles']//display_entry[4]/@icon">resourceAcid</set>
	
	<!-- Once workstation crafting skill reaches 50, set the probability bonus of finding workstation crafting skill magazines to 0 -->
	<set xpath="//perk[@name='perkAdvancedEngineering']/effect_group/passive_effect[11]/requirement[@name='ProgressionLevel']/@value">50</set>
	<set xpath="//perk[@name='perkAdvancedEngineering']/effect_group/passive_effect[15]/requirement[@name='ProgressionLevel']/@value">50</set>
	
	<!-- removes the prob bonus for electrician & trap magazines -->
	<remove xpath="//perk[@name='perkAdvancedEngineering']/effect_group//passive_effect[@tags='electricianCSM']"/>
	<remove xpath="//perk[@name='perkAdvancedEngineering']/effect_group//passive_effect[@tags='trapsCSM']"/>
	
	<!-- lowers number requirement of workstation magazines to craft workstations AFTER workbench, up to and including crucible -->
	<set xpath="//crafting_skill[@name='craftingWorkstations']/@max_level">50</set>
	<set xpath="/progression/crafting_skills/crafting_skill[@name='craftingWorkstations']">
		<display_entry icon="cntDewCollector,forge,resourceLockPick,workbench" name_key="workstationsT1-1,workstationsT1-3,workstationsT1-4,workstationsT1-5" has_quality="false" unlock_level="1,5,7,10">
			<unlock_entry item="cntDewCollector" unlock_tier="1"/>
			<unlock_entry item="forge" unlock_tier="2"/>
			<unlock_entry item="resourceLockPick" unlock_tier="3"/>
			<unlock_entry item="workbench" unlock_tier="4"/>
			
		</display_entry>
		<display_entry icon="toolBellows,toolAnvil,toolDewGatherer" name_key="workstationsT2-1,workstationsT2-3,workstationsT2-6" has_quality="false" unlock_level="16,20,26">
			<unlock_entry item="toolBellows" unlock_tier="1"/>
			<unlock_entry item="toolAnvil" unlock_tier="2"/>
			<unlock_entry item="toolDewGatherer" unlock_tier="3"/>
		</display_entry>
		<display_entry icon="cntApiary,toolDewTarp,cementMixer" name_key="workstationsT3-2,workstationsT3-3,workstationsT3-4" has_quality="false" unlock_level="27,28,30">
			<unlock_entry item="cntApiary" unlock_tier="1"/>
			<unlock_entry item="toolDewTarp" unlock_tier="2"/>
			<unlock_entry item="cementMixer" unlock_tier="3"/>
		</display_entry>
		<display_entry icon="toolApiaryExtractor,toolApiaryBroodBox,resourceWaterFilter,chemistryStation" name_key="workstationsT4-1,workstationsT4-3,workstationsT4-5,workstationsT4-6" has_quality="false" unlock_level="32,34,36,40">
			<unlock_entry item="toolApiaryExtractor" unlock_tier="1"/>
			<unlock_entry item="toolApiaryBroodBox" unlock_tier="2"/>
			<unlock_entry item="toolDewFilter" unlock_tier="3"/>
			<unlock_entry item="chemistryStation" unlock_tier="4"/>
		</display_entry>
		<display_entry icon="toolApiarySmoker,toolForgeCrucible" name_key="workstationsT5-2,workstationsT5-5" has_quality="false" unlock_level="45,50">
			<unlock_entry item="toolApiarySmoker" unlock_tier="1"/>
			<unlock_entry item="toolForgeCrucible" unlock_tier="2"/>
		</display_entry>

		<effect_group>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="1,50" value="1" tags="cntDewCollector"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="5,50" value="1" tags="forge"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="7,50" value="1" tags="resourceLockPick"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="10,50" value="1" tags="workbench"/>			
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="16,50" value="1" tags="toolBellows"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="20,50" value="1" tags="toolAnvil"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="26,50" value="1" tags="toolDewGatherer"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="27,50" value="1" tags="cntApiary"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="28,50" value="1" tags="toolDewTarp"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="30,50" value="1" tags="cementMixer"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="32,50" value="1" tags="toolApiaryExtractor"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="34,50" value="1" tags="toolApiaryBroodBox"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="36,50" value="1" tags="toolDewFilter"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="40,50" value="1" tags="chemistryStation"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="45,50" value="1" tags="toolApiarySmoker"/>
			<passive_effect name="RecipeTagUnlocked" operation="base_set" level="50,50" value="1" tags="toolForgeCrucible"/>
		</effect_group>
	</set>
	
	<remove xpath="//skill[@name='skillIntellectInfluence']"/>
	<remove xpath="//perk[@name='perkBetterBarter']"/>
	<remove xpath="//perk[@name='perkDaringAdventurer']"/>
	<remove xpath="//perk[@name='perkCharismaticNature']"/>
	
	<!-- new tags and progression for looting zombie corpses -->
	<insertbefore xpath="//perk[@name='perkGreaseMonkey']/effect_group//effect_description[1]">
		<passive_effect name="LootQuantity" operation="perc_add" level="1,2,3,4,5" value="1,2,3,4,5" tags="vehiclesCSM">
			<requirement name="ProgressionLevel" progression_name="craftingVehicles" operation="LT" value="50"/>
		</passive_effect>
		
		<passive_effect name="LootQuantity" operation="base_set" level="1,2,3,4,5" value="0,0,0,0,0" tags="vehiclesCSM">
			<requirement name="ProgressionLevel" progression_name="craftingVehicles" operation="equals" value="50"/>
		</passive_effect>
	</insertbefore>	
</configs>